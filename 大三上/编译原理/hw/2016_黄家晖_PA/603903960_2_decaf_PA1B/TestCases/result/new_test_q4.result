                vardef b inttype
                vardef c inttype
                assign
                    varref a
                    intconst 0
                assign
                    varref b
                    intconst 1
                assign
                    varref c
                    intconst 2
                repeat
                    gtr
                        varref a
                        varref b
                    stmtblock
                        if
